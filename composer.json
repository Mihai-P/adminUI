{
  "name": "tez/yii2theme-adminui",
  "description": "Yii2 admin ui based on AdminLTE free template",
  "version":"1.0.5",
  "keywords": [
    "yii2",
	"theme",
    "adminUi",
	"layout"
  ],
  "type": "yii2-extension",
  "license": "GPLv2",
  "support": {
    "issues": "https://github.com/Mihai-p/yii2theme-adminUI/issues?page=1&state=open",
    "source": "https://github.com/Mihai-p/yii2theme-adminUI"
  },
  "authors": [
    {
      "name": "Mihai Petrescu",
	  "email":"mihai.petrescu@gmail.com",
      "homepage": "http://2ezweb.com.au"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2":">= 2.0.0",
    "yiisoft/yii2-bootstrap": ">= 2.0.0",
    "bower-assets/font-awesome": "4.2.0",
    "bower-assets/Ionicons":"1.5.2",
    "bower-assets/ckeditor":"4.*",
    "bower-assets/bootstrap-daterangepicker":"1.3.13",
    "bower-assets/tinymce":"4.*",
    "bower-assets/dropzone":"3.*",
    "bower-assets/jquery-ui":"1.11.2",
    "almasaeed2010/adminlte":"2.*"
  },
  "autoload": {
    "psr-4": {
      "tez\\theme\\": ""
    }
  },
  "extra":{
	"bootstrap":"tez\\theme\\AdminLTEBootstrap"
  }
}
